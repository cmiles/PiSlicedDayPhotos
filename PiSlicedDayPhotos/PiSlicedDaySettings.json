{
  "DaySlices": 2,
  "NightSlices": 2,
  "PhotoStorageDirectory": "../SlicedPhotos",
  "SunriseSunsetCsvFile": "SunriseAndSunset.csv",
  "PhotoSeriesName": "PiSlice",
  "LogFullExceptionsToImages": true,
  "LibCameraParametersDay": "--autofocus-mode manual --lens-position 0 --awb auto --metering average --denoise cdn_hq",
  "LibCameraParametersNight": "--autofocus-mode manual --lens-position 0 --shutter 60000000 --awb auto --denoise cdn_hq",
  "LibCameraParametersSunset": "--autofocus-mode manual --lens-position 0 --ev -2 --awb auto --metering average --denoise cdn_hq",
  "LibCameraParametersSunrise": "--autofocus-mode manual --lens-position 0  --awb auto --metering average --denoise cdn_hq",
  "CustomTimes": [
    {
      "Time": "Sunset+10",
      "LibCameraParameters": "--autofocus-mode manual --lens-position 0 --awb auto --metering average --denoise cdn_hq"
    },
    {
      "Time": "Sunrise-10",
      "LibCameraParameters": "--autofocus-mode manual --lens-position 0 --awb auto --metering average --denoise cdn_hq"
    }
  ]
}
